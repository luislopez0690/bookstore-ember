<div class="card">
  <button {{action "toggleModal"}}>Toggle Modal</button>
  <img class="card-img-top" src="http://via.placeholder.com/350x250" alt="Card image cap">
  <div class="card-body">
    <h5 class="card-title">{{book.name}}</h5>
    <em>by</em>
    <p class="card-text">{{book.author}}</p>
  </div>
</div>
