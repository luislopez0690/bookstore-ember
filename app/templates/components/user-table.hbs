
<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Author</th>
      <th scope="col">Action</th>
      <th scope="col">Amount</th>
    </tr>
  </thead>
  <tbody>
    {{#each model.user.books as |book|}}
      <tr>
        <td>{{book.name}}</td>
        <td>{{book.author}}</td>
        <td>1</td>
        <td><button type="button" class="btn btn-secondary"{{action 'deleteFromUserLibrary' book}} > Return Order </button></td>
        </tr>
    {{/each}}
  </tbody>
</table>
